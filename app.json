{
  "expo": {
    "name": "Bar app",
    "slug": "bar-app",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/black.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "ts", "tsx", "svg"]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.alepacheco.expozeit",
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSFaceIDUsageDescription": "FaceID to confirm purchases"
      }
    },
    "android": {
      "package": "dev.alepacheco.expozeit"
    }
  }
}
