{
  "name": "barapp-app",
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "builds": [
    {
      "src": "/api/**",
      "use": "@now/node"
    }
  ],
  "build": {
    "env": {
      "IS_NOW": "true",
      "ENV": "PROD"
    }
  },
  "env": {
    "IS_NOW": "true",
    "ENV": "PROD"
  }
}
