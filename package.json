{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build-web": "expo-cli build:web",
    "eject": "expo eject",
    "publish": "expo publish",
    "test": "jest && tsc && npm run lint",
    "prettier-check": "prettier --check \"**/*.ts\" --check  \"**/*.tsx\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-pattern node_modules/",
    "fix": "npm run prettier && npm run lint -- --fix",
    "prettier": "prettier --write \"**/*.ts\" \"**/*.tsx\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "easy-peasy": "^3.2.3",
    "expo": "^36.0.0",
    "expo-blur": "~8.0.0",
    "expo-file-system": "~8.0.0",
    "expo-haptics": "~8.0.0",
    "expo-image-picker": "~8.0.1",
    "expo-location": "~8.0.0",
    "expo-secure-store": "~8.0.0",
    "lottie-react-native": "~2.6.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.1.tar.gz",
    "react-native-appearance": "~0.3.1",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-maps": "0.26.1",
    "react-native-reanimated": "~1.4.0",
    "react-native-svg": "9.13.3",
    "react-native-web": "^0.11.7",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.3",
    "react-navigation-tabs": "^2.5.6",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@now/node": "^1.0.1",
    "@testing-library/react-native": "^5.0.0",
    "@types/jest": "^24.0.25",
    "@types/react": "^16.9.5",
    "@types/react-native": "^0.60.19",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^16.9.0",
    "@types/styled-components": "^4.1.22",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "babel-preset-expo": "^7.0.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "jest": "^24.9.0",
    "jest-expo": "^36.0.0",
    "prettier": "^1.19.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-svg-transformer": "^0.13.0",
    "react-test-renderer": "^16.10.2",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "private": true,
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "singleQuote": true
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base|unimodules-permissions-interface)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx",
      "node"
    ]
  }
}
